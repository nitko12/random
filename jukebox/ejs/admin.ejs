          <script>
            var modal = document.getElementById("myModal");
            var modal_text = document.getElementById("myModal_text");
            var modal_volume = document.getElementById("myModal_volume");
            var modal_sat = document.getElementById("myModal_sat");
            var modal_zaustavi = document.getElementById("myModal_zaustavi");

            var btn = document.getElementById("myBtn");
            var btn_text = document.getElementById("myBtn_text");
            var btn_volume = document.getElementById("myBtn_volume");
            var btn_sat = document.getElementById("myBtn_sat");
            var btn_zaustavi = document.getElementById("myBtn_zaustavi");

            var span = document.getElementsByClassName("close")[0];
            var span_text = document.getElementsByClassName("close")[1];
            var span_volume = document.getElementsByClassName("close")[2];
            var span_sat = document.getElementsByClassName("close")[3];
            var span_zaustavi = document.getElementsByClassName("close")[4];

            btn.onclick = function() {
              modal.style.display = "block";
            };
            btn_text.onclick = function() {
              modal_text.style.display = "block";
            };
            btn_volume.onclick = function() {
              modal_volume.style.display = "block";
            };
            btn_sat.onclick = function() {
              modal_sat.style.display = "block";
            };
            btn_zaustavi.onclick = function() {
              modal_zaustavi.style.display = "block";
            };

            span.onclick = function() {
              modal.style.display = "none";
            };
            span_text.onclick = function() {
              modal_text.style.display = "none";
            };
            span_volume.onclick = function() {
              modal_volume.style.display = "none";
            };
            span_sat.onclick = function() {
              modal_sat.style.display = "none";
            };
            span_zaustavi.onclick = function() {
              modal_zaustavi.style.display = "none";
            };

            window.onclick = function(event) {
              if (event.target == modal) {
                modal.style.display = "none";
              }
              if (event.target == modal_text) {
                modal_text.style.display = "none";
              }
              if (event.target == modal_volume) {
                modal_volume.style.display = "none";
              }
              if (event.target == modal_sat) {
                modal_sat.style.display = "none";
              }
              if (event.target == modal_zaustavi) {
                modal_zaustavi.style.display = "none";
              }
            };

            document.addEventListener("keyup", function(e) {
              if (e.keyCode == 27 || e.keyCode == 13) {
                modal.style.display = "none";
                modal_text.style.display = "none";
                modal_volume.style.display = "none";
                modal_sat.style.display = "none";
                modal_zaustavi.style.display = "none";
              }
            });
          </script>
